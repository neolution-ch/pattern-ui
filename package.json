{
  "name": "@neolution-ch/react-pattern-ui",
  "version": "1.0.10",
  "description": "Some basic functions and components which are needed in most of projects.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neolution-ch/react-pattern-ui"
  },
  "license": "MIT",
  "author": "Neolution",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "shx rm -rf dist && microbundle --no-compress --tsconfig ./tsconfig.microbundle.json",
    "lint": "eslint \"**/*.{ts,tsx}\" --cache --max-warnings 0",
    "prepack": "yarn build",
    "prettier-check": "prettier --check .",
    "start": "microbundle watch --no-compress --tsconfig ./tsconfig.microbundle.json",
    "start-all": "concurrently \"yarn start\" \"yarn start-yalc\"",
    "start-yalc": "yarn nodemon --watch dist -x \"yarn yalc push\"",
    "test": "cross-env DEBUG_PRINT_LIMIT=100 jest",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/reactstrap": "^8.7.2",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^27.1.4",
    "microbundle": "^0.14.2",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reactstrap": "^8.0.0",
    "shx": "^0.3.3",
    "ts-jest": "^27.1.4",
    "typescript": "^4.2.3",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reactstrap": "^8.0.0"
  },
  "engines": {
    "node": ">=10"
  }
}
